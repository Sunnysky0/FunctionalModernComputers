{
    "multipart": [
        {   "apply": { "model": "fmc:duct_noside" }},
        {   "when": { "north": true },
            "apply": { "model": "fmc:duct_side" }
        },
        {   "when": { "east": true },
            "apply": { "model": "fmc:duct_side", "y": 90 }
        },
        {   "when": { "south": true },
            "apply": { "model": "fmc:duct_side_alt" }
        },
        {   "when": { "west": true },
            "apply": { "model": "fmc:duct_side_alt", "y": 90 }
        },
        {   "when": { "up": true },
            "apply": { "model": "fmc:duct_side", "x": 270 }
        },
        {   "when": { "down": true },
            "apply": { "model": "fmc:duct_side", "x": 90 }
        }
    ]
}
